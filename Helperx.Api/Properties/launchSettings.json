{
  "profiles": {
    "Helperx.Api": {
      "commandName": "Project",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "https://localhost:7175;http://localhost:5126"
    },
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}",
      "publishAllPorts": true,
      "useSSL": true
    },
    "AddMigration": {
      "commandName": "Executable",
      "executablePath": "dotnet",
      "commandLineArgs": "ef migrations add inittial --project ../Helperz.Infra.Data",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "workingDirectory": "$(MSBuildProjectDirectory)"
    },
    "UpdateDB": {
      "commandName": "Executable",
      "executablePath": "dotnet",
      "commandLineArgs": "ef database update --project ../Helperz.Infra.Data",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "workingDirectory": "$(MSBuildProjectDirectory)"
    },
    "DropDB": {
      "commandName": "Executable",
      "executablePath": "dotnet",
      "commandLineArgs": "ef database drop -f",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "workingDirectory": "$(MSBuildProjectDirectory)"
    }
  },
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:25929",
      "sslPort": 44313
    }
  }
}